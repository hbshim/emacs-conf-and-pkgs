\newtheorem{exmp}[defn]{Example}
\newtheorem{xca}[defn]{Exercise}
\newtheorem{xcadefn}[defn]{Exercise-Definition}
\newtheorem{convention}[defn]{Convention}
\newtheorem{stmt}[defn]{}

%\theoremstyle{plain} % default style for amsthm; italic text, extra space above and below;
\newtheorem{lem}[defn]{Lemma}
\newtheorem{lemdefn}[defn]{Lemma-Definition}
\newtheorem{prop}[defn]{Proposition}
\newtheorem{propdefn}[defn]{Proposition-Definition}
\newtheorem{thm}[defn]{Theorem}
\newtheorem{thmdefn}[defn]{Theorem-Definition}
\newtheorem{cor}[defn]{Corollary}
\newtheorem{cordefn}[defn]{Corollary-Definition}
\newtheorem{conj}[defn]{Conjecture}
\newtheorem{assert}[defn]{Assertion}

