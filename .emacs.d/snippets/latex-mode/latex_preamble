% The followings define mdframed styles. use as \begin{mdframed}[style=note] ...
\mdfdefinestyle{note}{ %
  hidealllines=true,
  backgroundcolor=red!7.5,
  innerleftmargin=0pt,
  innerrightmargin=0pt,
  innertopmargin=1pt,
  innerbottommargin=1pt}

\mdfdefinestyle{boxed}{ %
  middlelinewidth = 2,
  middlelinecolor=blue!25,
  innerleftmargin=0pt,
  innerrightmargin=0pt,
  innertopmargin=1pt,
  innerbottommargin=1pt}

\mdfdefinestyle{boxednote}{ %
  backgroundcolor=red!7.5,
  middlelinewidth = 2,
  middlelinecolor=blue!25,
  innerleftmargin=0pt,
  innerrightmargin=0pt,
  innertopmargin=1pt,
  innerbottommargin=1pt}

\mdfdefinestyle{graybox}{ %
  backgroundcolor=gray!30,
  leftmargin=-1em,
  rightmargin=-1em,
  linewidth=0pt,}

